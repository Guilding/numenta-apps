{
  "$schema": "http://json-schema.org/schema#",
  "id": "/db/MetricData",
  "type": "object",
  "properties": {
    "metric_uid": {
      "type": "string",
      "maxLength": 50,
      "minLength": 1,
      "required": true
    },
    "iso_timestamp": {
      "type": "string",
      "minLength": 10,
      "maxLength": 32,
      "required": true
    },
    "naive_time": {
      "type": "integer",
      "required": true
    },
    "metric_value": {
      "type": "number",
      "required": true
    }
  },
  "additionalProperties": false
}
